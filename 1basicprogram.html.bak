<!doctype html>
<html lang="en">
 <head>
 
  <title>Welocome to Gmail</title>
 </head>
 <body>
 <form>

 <label>Vehile Number</label>
 <input pattern="[A-Z]{2}[0-9]{2}[A-Z]{1,2}[0-9]{4}">
<br></br>
 <label>Card Number</label>
     <input pattern="[1-9]{1}[0-9]{15}"></input>

	 <br></br>
 
  <label>Phone Number : </label>
     <input pattern="[+]{1}[0-9]{1,3}[6-9]{1}[0-9]{9}" ></input>

     <br></br>

  <label>DOB : </label>
     <input type="date" ></input>

     <br></br>
	   
  <label>CVV : </label>
      <input type="password" pattern="[0-9]{3}"></input>

     <br></br>
  <label>ExpiryDate : </label>
      <input pattern="[0-1]{1}[0-9]{1}[/]{1}[0-9]{2}" title="Enter the details in Month and year"></input>

      <br></br>
	  <label> Name on the card</label>
	  <input pattern="[A-Z]{4,20}"></input>
	  <br></br>

   <label> Gender</label>
   <input type="radio" name=gender>MALE</input>
   <input type="radio" name=gender>FEMALE</input>
   <input type="radio" name=gender>OTHERS</input>

   <br></br>

    <label> Percentage</label>
   <input type="radio" name=percent>above 90</input>
   <input type="radio" name=percent>above 80</input>
   <input type="radio" name=percent>70 and below</input>
   <br></br>

   <label>IFSC code</label>
   <input pattern="[A-Z]{4}[0-9]{7}"></input>
    
	<br></br>

   <label>Upi Pin</label>
   <input type="password" pattern="[0-9]{4,6]"></input>

   <br></br>

   <input type="submit" value="check"></input>

</form>


